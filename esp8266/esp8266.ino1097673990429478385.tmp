void setup() {
  Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
  Serial.println("Hello world UNO!!");
  if(Serial.available ()){
    String RxBuffer = "";
    while(Serial.available){
      RxBuffer = Serial.readString();
    }
    Serial.println(RxBuffer)
  }
  delay(1000);
}
